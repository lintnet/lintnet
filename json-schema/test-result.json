{
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "additionalProperties": false,
   "description": "Test results",
   "items": {
      "description": "data file",
      "properties": {
         "data_file": {
            "description": "data file path. This overrides param.data's text, value and param.data.text",
            "type": "string"
         },
         "data_files": {
            "items": {
               "oneOf": [
                  {
                     "description": "data file path",
                     "type": "string"
                  },
                  {
                     "additionalProperties": false,
                     "description": "data file path",
                     "properties": {
                        "fake_path": {
                           "description": "Fake data file path. This is useful if a lint rule depends on data file path and you want to specify the different data file path from the actual file path",
                           "type": "string"
                        },
                        "path": {
                           "description": "data file path",
                           "type": "string"
                        }
                     },
                     "required": [
                        "path"
                     ],
                     "type": "object"
                  }
               ]
            },
            "type": "array"
         },
         "fake_data_file": {
            "description": "Fake data file path. This is useful if a lint rule depends on data file path and you want to specify the different data file path from the actual file path",
            "type": "string"
         },
         "name": {
            "description": "test name",
            "type": "string"
         },
         "param": {
            "additionalProperties": false,
            "description": "Top level arguments",
            "properties": {
               "combined_data": {
                  "description": "A list of data. This is set if the lint file is a combined lint file",
                  "items": {
                     "additionalProperties": false,
                     "description": "data file",
                     "properties": {
                        "file_path": {
                           "description": "data file path",
                           "type": "string"
                        },
                        "file_type": {
                           "description": "data file type",
                           "enum": [
                              "csv",
                              "hcl2",
                              "json",
                              "plain_text",
                              "toml",
                              "tsv",
                              "yaml"
                           ],
                           "type": "string"
                        },
                        "text": {
                           "description": "data file content",
                           "type": "string"
                        },
                        "value": {
                           "description": "data file content"
                        }
                     },
                     "required": [
                        "file_path",
                        "text",
                        "value",
                        "file_type"
                     ],
                     "type": "object"
                  },
                  "type": "array"
               },
               "config": {
                  "additionalProperties": true,
                  "description": "configuration",
                  "type": "object"
               },
               "data": {
                  "additionalProperties": false,
                  "description": "data file",
                  "properties": {
                     "file_path": {
                        "description": "data file path",
                        "type": "string"
                     },
                     "file_type": {
                        "description": "data file type",
                        "enum": [
                           "csv",
                           "hcl2",
                           "json",
                           "plain_text",
                           "toml",
                           "tsv",
                           "yaml"
                        ],
                        "type": "string"
                     },
                     "text": {
                        "description": "data file content",
                        "type": "string"
                     },
                     "value": {
                        "description": "data file content"
                     }
                  },
                  "required": [
                     "file_path",
                     "text",
                     "value",
                     "file_type"
                  ],
                  "type": "object"
               }
            },
            "required": [
               "config"
            ],
            "type": "object"
         },
         "result": {
            "description": "results",
            "items": {
               "additionalProperties": false,
               "description": "result",
               "properties": {
                  "custom": {
                     "additionalProperties": true,
                     "description": "Custom fields that users can set freely",
                     "type": "object"
                  },
                  "level": {
                     "description": "error level",
                     "enum": [
                        "debug",
                        "info",
                        "warn",
                        "error"
                     ],
                     "type": "string"
                  },
                  "links": {
                     "oneOf": [
                        {
                           "additionalProperties": {
                              "description": "link",
                              "type": "string"
                           },
                           "description": "each key is a link title",
                           "type": "object"
                        },
                        {
                           "items": {
                              "oneOf": [
                                 {
                                    "description": "link",
                                    "type": "string"
                                 },
                                 {
                                    "additionalProperties": false,
                                    "description": "result",
                                    "properties": {
                                       "link": {
                                          "description": "link",
                                          "type": "string"
                                       },
                                       "title": {
                                          "description": "link title",
                                          "type": "string"
                                       }
                                    },
                                    "required": [
                                       "link"
                                    ],
                                    "type": "object"
                                 }
                              ]
                           },
                           "type": "array"
                        }
                     ]
                  },
                  "location": {
                     "oneOf": [
                        {
                           "additionalProperties": true,
                           "description": "Location where errors occur",
                           "type": "object"
                        },
                        {
                           "description": "Location where errors occur",
                           "type": "string"
                        }
                     ]
                  },
                  "message": {
                     "description": "error message",
                     "type": "string"
                  },
                  "name": {
                     "description": "rule name",
                     "type": "string"
                  }
               },
               "required": [
                  "name"
               ],
               "type": "object"
            },
            "type": "array"
         }
      },
      "required": [
         "name",
         "param",
         "result"
      ],
      "type": "object"
   },
   "type": "array"
}
